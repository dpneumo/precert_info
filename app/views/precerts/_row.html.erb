  <tr class='<%= background %>' >
    <td class='td-pad'>
      <%= link_to item.id,
                  precert_path(item),
                  class: "td-emphasized link-hover" %>
    </td>
    <td class='td-pad td-base hidden lg:table-cell'><%= item.status %></td>
    <td class='td-pad td-base'><%= item.patientMRN %></td>
    <td class='td-pad td-base'><%= item.provider.name %></td>
    <td class='td-pad td-base hidden sm:table-cell'><%= item.diagnosis.name %></td>
    <td class='td-pad td-base'><%= item.service.name %></td>
    <td class='td-pad td-base hidden md:table-cell'><%= item.insurer.name %></td>
    <td class='td-pad td-base hidden lg:table-cell'><%= item.fmtd_submit_date %></td>
    <td class='td-pad td-base'><%= item.approved %></td>
    <td class='td-pad td-base hidden xl:table-cell'><%= item.confirm_hint %></td>
    <td class='td-pad td-base hidden xl:table-cell'><%= item.fmtd_approval_date %></td>
    <td class='td-pad td-base hidden xl:table-cell'><%= item.fmtd_closed_date %></td>
    <td class='td-pad td-base hidden xl:table-cell'><%= item.note_hint %></td>
  </tr>
