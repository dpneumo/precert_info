  <tr class='<%= background %>' >
    <td class='td-pad'>
      <%= link_to item.id,
                  precert_path(item),
                  class: "td-emphasized link-hover" %>
    </td>
    <td class='td-pad td-base'><%= item.patientMRN %></td>
    <td class='td-pad td-base'><%= item.diagnosis %></td>
    <td class='td-pad td-base'><%= item.insurer %></td>
    <td class='td-pad td-base'><%= item.provider %></td>
    <td class='td-pad td-base'><%= item.service %></td>
    <td class='td-pad td-base'><%= item.fmtd_submit_date %></td>
    <td class='td-pad td-base hidden lg:table-cell'><%= item.fmtd_approve_date %></td>
    <td class='td-pad td-base hidden lg:table-cell'><%= item.confirmation %></td>
    <td class='td-pad td-base hidden lg:table-cell'><%= item.note_hint %></td>
  </tr>
